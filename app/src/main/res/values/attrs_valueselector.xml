<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TestValueSelector">

        <!---->
        <attr name="testvalueColor" format="color" />
        <attr name="testborderColor" format="color" />
        <attr name="testplusBtnColor" format="color" />
        <attr name="testminusBtnColor" format="color" />
        <attr name="testactionDownColor" format="color" />
        <!---->

        <!---->
        <attr name="testminValue" format="integer" />
        <attr name="testmaxValue" format="integer" />
        <attr name="teststartValue" format="integer" />
        <attr name="testupdateInterval" format="integer" />
        <attr name="testgapValue" format="integer" />
        <!---->

        <!---->
        <attr name="testborderThickness" format="integer" />
        <attr name="testborderRadius" format="integer" />
        <!---->

        <!---->
        <attr name="testvalueTextSize" format="dimension" />
        <attr name="testplusIconWidthSize" format="dimension" />
        <attr name="testplusIconHeightSize" format="dimension" />
        <attr name="testminusIconWidthSize" format="dimension" />
        <attr name="testminusIconHeightSize" format="dimension" />
        <!---->

        <attr name="testcustomFontFamily" format="string" />


        <attr name="testvalueSelectorOrientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <attr name="testvalueSelectorFontFamily">
            <enum name="monospace" value="0" />
            <enum name="serif" value="1" />
            <enum name="sans_serif" value="2" />
        </attr>

        <attr name="testinvertIconsPlace">
            <enum name="False" value="0" />
            <enum name="True" value="1" />
        </attr>

        <attr name="testiconTypes">
            <enum name="plus_minus" value="0" />
            <enum name="arrow" value="1" />
            <enum name="expand" value="2" />
            <enum name="circle" value="3" />
        </attr>


    </declare-styleable>

</resources>